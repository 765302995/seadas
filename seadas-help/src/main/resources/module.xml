<module>
    <manifestVersion>1.0.0</manifestVersion>

    <symbolicName>seadas-help</symbolicName>
    <name>SeaDAS Help</name>
    <version>7.0</version>
    <packaging>dir</packaging>


    <description>Provides the software user manual.</description>

    <vendor>NASA/GSFC Ocean Biology Processing Group</vendor>
    <url>http://seadas.gsfc.nasa.gov/</url>
    <licenseUrl>http://www.gnu.org/licenses/gpl.html</licenseUrl>

    <changelog>
        <![CDATA[

        ]]>
    </changelog>

    <categories>System,Help</categories>

    <dependency>
        <module>beam-ui</module>
    </dependency>

    <extension point="beam-ui:helpSets">
        <helpSet>
            <id>seadas-help</id>
            <path>doc/help/seadas.hs</path>
        </helpSet>
    </extension>

</module>