<?xml version="1.0" encoding="ISO-8859-1"?>
<paramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="http://seadas.gsfc.nasa.gov/software/schemas/ParamInfo-1.0.xsd">
    <programMetaData>
        <hasParFile>true</hasParFile>
        <parFileOptionName>par</parFileOptionName>
        <primaryOptions>
            <primaryOption>ifile</primaryOption>
            <primaryOption>geofile</primaryOption>
            <primaryOption>ofile</primaryOption>
        </primaryOptions>
    </programMetaData>
    <options>
        <option type="ifile">
            <name>ifile</name>
            <value></value>
            <default></default>
            <description>input L1 file name</description>
            <source>command line</source>
            <aliases>
                <alias>ifile1</alias>
            </aliases>
            <order>1</order>
        </option>
        <option type="ofile">
            <name>ofile</name>
            <value></value>
            <default>output</default>
            <description>output L2 file #1 name, output vicarious L1B for inverse mode</description>
            <alias>ofile1</alias>
            <order>2</order>
        </option>
        <option type="int">
            <name>spixl</name>
            <value>1</value>
            <default>1</default>
            <description>start pixel number</description>
            <order>3</order>
        </option>
        <option type="int">
            <name>epixl</name>
            <value>-1</value>
            <default>-1</default>
            <description>end pixel number (-1=the last pixel)</description>
            <order>4</order>
        </option>
        <option type="int">
            <name>sline</name>
            <value>1</value>
            <default>1</default>
            <description>start line number</description>
            <order>5</order>
        </option>
        <option type="int">
            <name>eline</name>
            <value>-1</value>
            <default>-1</default>
            <description>end line number (-1=the last line)</description>
            <order>6</order>
        </option>
        <option type="ifile">
            <name>calfile</name>
            <value></value>
            <default></default>
            <description>system calibration file</description>
            <order>7</order>
        </option>
        <option type="ifile">
            <name>xcalfile</name>
            <value></value>
            <default></default>
            <description>cross-calibration file</description>
            <order>8</order>
        </option>

        <option type="float">
            <name>gain</name>
            <value></value>
            <default>output</default>
            <description>calibration gain multiplier</description>
            <order>9</order>
        </option>
               <option type="float">
            <name>offset</name>
            <value></value>
            <default>output</default>
            <description>calibration offset adjustment</description>
            <order>10</order>
        </option>
               <option type="int">
            <name>sl_pixl</name>
            <value>-1</value>
            <default>-1</default>
            <description>SeaWiFS only, number of LAC pixels for straylight flagging </description>
            <order>11</order>
        </option>
        <option type="float">
            <name>sl_frac</name>
            <value>0.25</value>
            <default>0.25</default>
            <description> = SeaWiFS only, straylight fractional threshold on Ltypical </description>
            <order>12</order>
        </option>
    </options>
</paramInfo>
