<?xml version="1.0" encoding="ISO-8859-1"?>

<param_info xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:noNamespaceSchemaLocation="paramInfo_type_nodes.xsd">
    <program_meta_data>
        <has_param_file>false</has_param_file>
        <par_file_option_name>NO_PAR_FILE</par_file_option_name>
    </program_meta_data>
    <option>
        <string_option>
            <name>ifile</name>
            <value></value>
            <type>string</type>
            <description>input MODIS L1A datafile</description>
            <source>command line</source>
            <alias>ifile1</alias>
            <order>0</order>
        </string_option>
    </option>
    <option>
        <int_option>
            <name>spixl</name>
            <value>1</value>
            <type>int</type>
            <default>1</default>
            <description>start pixel number</description>
            <order>1</order>
        </int_option>
    </option>
    <option>
        <int_option>
            <name>epixl</name>
            <value>-1</value>
            <type>int</type>
            <default>-1</default>
            <description>end pixel number (-1=the last pixel)</description>
            <order>2</order>
        </int_option>
    </option>
    <option>
        <int_option>
            <name>sline</name>
            <value>1</value>
            <type>int</type>
            <default>1</default>
            <description>start line number</description>
            <order>3</order>
        </int_option>
    </option>
    <option>
        <int_option>
            <name>eline</name>
            <value>-1</value>
            <type>int</type>
            <default>-1</default>
            <description>end line number (-1=the last line)</description>
            <order>4</order>
        </int_option>
    </option>
    <option>
        <string_option>
            <name>ofile</name>
            <value></value>
            <type>string</type>
            <default>output</default>
            <description>output L2 file name</description>
            <alias>ofile1</alias>
            <order>5</order>
        </string_option>
    </option>
</param_info>